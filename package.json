{
  "name": "shell",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "live-server dist/shell",
    "build": "npm run build:shell && npm run build:clients",
    "build:clients": "npm run build:client-a && npm run build:client-b",
    "build:client-a": "ng build --prod --project client-a --output-hashing none --vendor-chunk false --output-path dist/shell/client-a",
    "build:client-b": "ng build --prod --project client-b --output-hashing none --vendor-chunk false --output-path dist/shell/client-b",
    "build:shell": "ng build --project shell",
    "build:clients:webpack:old-solution": "webpack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "shell": "npm run serve:shell",
    "client-a": "npm run serve:client-a",
    "client-b": "npm run serve:client-b",
    "serve:shell": "ng serve --project shell --port 8200 --proxyConfig src/proxy.conf.json",
    "serve:client-a": "ng serve --project client-a --port 8201 --single-bundle --source-map",
    "serve:client-b": "ng serve --project client-b --port 8202 --single-bundle --source-map"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "~7.4.0",
    "@angular/animations": "~7.2.15",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/elements": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/http": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "@webcomponents/custom-elements": "^1.1.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "rxjs": "6.5.5",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/language-service": "~7.2.15",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cpr": "^3.0.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4",
    "webpack-cli": "^2.0.14",
    "ngx-build-plus": "^7.8.3",
    "@angular-devkit/build-angular": "^0.13.10"
  }
}
